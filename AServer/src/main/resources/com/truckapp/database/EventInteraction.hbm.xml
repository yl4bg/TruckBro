<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.truckapp.database">

    <class name="EventInteraction" table="eventinteractions">
        <composite-id name="eventInteractionCompositeId">
			<key-property name="eventId" column="eventid" />
			<key-property name="interaction" column="interaction" />
			<key-property name="userId" column="userid" />
		</composite-id>
    </class>

</hibernate-mapping>