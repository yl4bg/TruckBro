<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.truckapp.database">

    <class name="UserGoodType" table="usergoodtypes">
        <composite-id name="userGoodTypeCompositeId">
			<key-property name="userID" column="userid" />
			<key-property name="goodType" column="goodtype" />
		</composite-id>
    </class>

</hibernate-mapping>
